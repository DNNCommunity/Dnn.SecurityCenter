{
  "name": "dnn-securitycenter",
  "version": "1.3.0",
  "private": true,
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/dnn-securitycenter/dnn-securitycenter.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "eslint --fix --ext .ts --ext .tsx --ext .js --ignore-pattern **/*.d.ts ./src && stencil build --docs",
    "start": "stencil build --dev --watch --serve --no-open",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "devDependencies": {
    "@dnncommunity/dnn-elements": "^0.18.0",
    "@stencil/core": "^3.4.0",
    "@stencil/eslint-plugin": "^0.4.0",
    "@stencil/sass": "^3.0.4",
    "@stencil/store": "^2.0.9",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.1",
    "eslint": "^7.32.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "octokit": "^2.0.22",
    "typescript": "^4.9.5"
  },
  "license": "MIT"
}
